<%- model_class = ExportSet -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:record_size) %>:</strong></dt>
  <dd><%= @export_set.record_size %></dd>
  <dt><strong><%= model_class.human_attribute_name(:data_record_id) %>:</strong></dt>
  <dd><%= @export_set.data_record_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:export_run_id) %>:</strong></dt>
  <dd><%= @export_set.export_run_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              export_sets_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_export_set_path(@export_set), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              export_set_path(@export_set),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
